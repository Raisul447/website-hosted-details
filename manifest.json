{
  "manifest_version": 3,
  "name": "Website Hosted Details",
  "version": "1.0.3",
  "description": "Displays the website's Hosted IP address and geolocation, along with its coordinates on Google Maps.",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://dns.google/*",
    "https://ipwho.is/*"
  ],
  "action": {
    "default_title": "Website Hosted Details",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}